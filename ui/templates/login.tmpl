                <div class="content">
                    <div class="title">{{.ClientName}}</div>
                    <form action="/v1/authorize/handle" method="post">

                        <div class="formTitle">Login</div>

                        {{ if .IncorrectCredentials }}
                        <div class="alert">Incorrect credentials</div>
                        {{ end }}

                        <div class="form-input">
                            <input class="username" placeholder="Enter your username" type="text" name="username" autocorrect="off" autocomplete="off" spellcheck="false">
                        </div>
                        <div class="form-input">
                            <input class="password" placeholder="Enter your password" type="password" name="password">
                        </div>

                        <input type="hidden" name="redirect_uri" value="{{.RedirectURI}}">
                        <input type="hidden" name="client_id" value="{{.ClientID}}">
                        <input type="hidden" name="auth_session_id" value="{{.AuthSessionID}}">

                        {{if .ClientDescription}}
                            <div class="description">{{.ClientDescription}}</div>
                        {{end}}

                        <button class="form-submit" type="submit">Login</button>
                        <div class="redirect-info">
                            <div class="redirect-info-label">Authenticating will redirect to</div>
                            <div class="redirect-info-uri">{{.RedirectURI}}</div>
                        </div>
                    </form>
                </div>
